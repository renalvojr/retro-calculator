{
  "manifest_version": 2,
  "name": "Retro Calculator",
  "version": "1.0",
  "description": "An easy and fast way to calculate while you browse",
  "icons": {
    "128": "images/calculator128.png",
    "48": "images/calculator32.png",
    "16": "images/calculator16.png"
  },
  "browser_action": {
    "default_icon": "images/calculator16.png",
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
